{
    "version": 2,
    "builds": [
      {
        "src": "api/Main.py",
        "use": "@vercel/python"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/Main.py"
      },
      {
        "src": "/(.*)",
        "dest": "public/$1"
      }
    ],
    "functions": {
      "api/*.py": {
        "runtime": "python3.9"
      }
    }
  }
  